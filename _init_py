import datetools
import tstools
import cash
import underlying
import basket
import weight


from dateutil import parser
from datetime import timedelta
import pandas as pd

x = [parser.parse("12/04/2014", dayfirst = None).date() + timedelta(i) for i in range(10)]
y = [parser.parse("12/04/2014", dayfirst = None).date() + timedelta(2*i) for i in range(10)]

ratets = pd.DataFrame({'rate':0.1}, index = x)

c = cash.CashAmount()
c.start_cash=100
c.interest_rates = ratets
c.calculate_cash(y)

print c.cash_value
